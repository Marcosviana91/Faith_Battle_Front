{
  "expo": {
    "name": "Faith_Batlle",
    "slug": "faith-battle",
    "version": "alpha-1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/Faith_Batlle.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Faith_Batlle.png",
      "resizeMode": "contain",
      "backgroundColor": "#040611"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Faith_Batlle.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.marcosvianadev.faithbattle",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Faith_Batlle.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f96c6919-e312-47d9-9e14-7a23e7ae858e"
      }
    },
    "owner": "marcosvianadev"
  }
}